                "RULE_REFERENCE": null,
                "FIELD_FORM_ASSOCIATION": null
            }
        },
        "city": {
            "title": "CITY",
            "description": "",
            "type": "string",
            "minLength": 0,
            "maxLength": 30,
            "pattern": "^[ -~]{0,30}$",
            "examples": ["Anytown"],
            "fec_spec": {
                "COL_SEQ": 16,
                "FIELD_DESCRIPTION": "CITY",
                "TYPE": "A/N-30",
                "REQUIRED": "X (error)",
                "AUTO_POPULATE": null,
                "SAMPLE_DATA": "Anytown",
