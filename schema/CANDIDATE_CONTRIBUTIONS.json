    "allOf": [
        {
            "if": {
                "properties": {
                    "election_code": {
                        "pattern": "^O\\d{4}$",
                        "type": "string"
                    }
                },
                "required": [
                    "election_code"
                ]
            },
            "then": {
                "required": [
                    "election_other_description"
                ],
                "properties": {
                    "election_other_description": {
                        "type": "string"
                    }
                }
            }
        },
        {
            "if": {
                "properties": {
                    "transaction_type_identifier": {
                        "const": "CONTRIBUTION_TO_CANDIDATE_VOID"
                    }
                },
                "required": [
                    "transaction_type_identifier"
                ]
            },
            "then": {
                "required": [
                    "expenditure_purpose_descrip"
                ],
                "properties": {
                    "expenditure_amount": {
                        "exclusiveMaximum": 0,
                        "type": "number"
                    },
                    "expenditure_purpose_descrip": {
                        "type": "string"
                    }
                }
            }
        },
        {
            "if": {
                "properties": {
                    "beneficiary_candidate_office": {
                        "const": "H"
                    }
                },
                "required": [
                    "beneficiary_candidate_office"
                ]
            },
            "then": {
                "required": [
                    "beneficiary_candidate_state",
                    "beneficiary_candidate_district"
                ],
                "properties": {
                    "beneficiary_candidate_state": {
                        "type": "string"
                    },
                    "beneficiary_candidate_district": {
                        "type": "string"
                    }
                }
            }
        },
        {
            "if": {
                "properties": {
                    "beneficiary_candidate_office": {
                        "const": "S"
                    }
                },
                "required": [
                    "beneficiary_candidate_office"
                ]
            },
            "then": {
                "required": [
                    "beneficiary_candidate_state"
                ],
                "properties": {
                    "beneficiary_candidate_state": {
                        "type": "string"
                    }
                }
            }
        }
