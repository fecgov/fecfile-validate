                "string",
                "null"
            ],
            "minLength": 0,
            "maxLength": 8,
            "pattern": "^[ -~]{0,8}$",
            "examples": [
                "SA17"
            ],
            "fec_spec": {
                "FIELD_DESCRIPTION": "BACK REFERENCE SCHED NAME",
                "TYPE": "A/N-8",
                "REQUIRED": null,
                "SAMPLE_DATA": "SA17",
                "VALUE_REFERENCE": "SA[line# ref]",
                "RULE_REFERENCE": "Ref to the Schedule that has the Related Record.  SA3L must be used \nwith the F3L",
                "FIELD_FORM_ASSOCIATION": null
            }
        },
        "entity_type": {
            "title": "ENTITY TYPE",
            "description": "",
            "const": "ORG",
            "examples": [
                "ORG"
            ],
            "fec_spec": {
                "FIELD_DESCRIPTION": "ENTITY TYPE",
                "TYPE": "A/N-3",
                "REQUIRED": "X (error)",
                "SAMPLE_DATA": "ORG",
                "VALUE_REFERENCE": "ORG only",
                "RULE_REFERENCE": null,
                "FIELD_FORM_ASSOCIATION": null
            }
        },
        "contributor_organization_name": {
            "title": "CONTRIBUTOR ORGANIZATION",
