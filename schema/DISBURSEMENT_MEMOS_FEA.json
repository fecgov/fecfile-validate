{
    "$schema": "https://json-schema.org/draft-07/schema#",
    "$id": "https://github.com/fecgov/fecfile-validate/blob/main/schema/DISBURSEMENT_MEMOS_FEA.json",
    "version": "8.3.0.1",
    "title": "FEC Disbursement Memos FEA",
    "description": "SCHEDULE B - Credit Card Memo for 100% Federal Election Activity (Line 30b), Staff Reimbursement Memo for 100% Federal Election Activity (Line 30b), Payroll Memo for 100% Federal Election Activity (Line 30b)",
    "type": "object",
    "required": [
        "form_type",
        "filer_committee_id_number",
        "transaction_type_identifier",
        "transaction_id",
        "back_reference_tran_id_number",
        "back_reference_sched_name",
        "entity_type",
        "payee_street_1",
        "payee_city",
        "payee_state",
        "payee_zip",
        "election_code",
        "expenditure_date",
        "expenditure_amount",
        "aggregate_amount",
        "aggregation_group",
        "expenditure_purpose_descrip",
        "memo_code"
    ],
    "properties": {
        "form_type": {
            "title": "FORM TYPE",
            "description": "",
            "const": "SB30B",
            "examples": [
                "SB30B"
            ],
            "fec_spec": {
                "FIELD_DESCRIPTION": "FORM TYPE",
                "TYPE": "A/N-8",
                "REQUIRED": "X (error)",
                "SAMPLE_DATA": "SB30B",
                "VALUE_REFERENCE": "SB30B",
                "RULE_REFERENCE": "SB30B Only"
            }
        },
        "filer_committee_id_number": {
            "title": "FILER COMMITTEE ID NUMBER",
            "description": "",
            "type": "string",
            "minLength": 9,
            "maxLength": 9,
            "pattern": "^(?:[PC][0-9]{8}|[HS][0-9]{1}[A-Z]{2}[0-9]{5})$",
            "examples": [
                "C00123456"
            ],
            "fec_spec": {
                "FIELD_DESCRIPTION": "FILER COMMITTEE ID NUMBER",
                "TYPE": "A/N-9",
                "REQUIRED": "X (error)",
                "SAMPLE_DATA": "C00123456"
            }
        },
        "transaction_type_identifier": {
            "title": "TRANSACTION TYPE IDENTIFIER",
            "description": "",
            "enum": [
                "FEDERAL_ELECTION_ACTIVITY_CREDIT_CARD_PAYMENT_MEMO",
                "FEDERAL_ELECTION_ACTIVITY_STAFF_REIMBURSEMENT_MEMO",
                "FEDERAL_ELECTION_ACTIVITY_PAYMENT_TO_PAYROLL_MEMO"
            ],
            "examples": [
                "FEDERAL_ELECTION_ACTIVITY_CREDIT_CARD_PAYMENT_MEMO"
            ],
            "fec_spec": {
                "FIELD_DESCRIPTION": "TRANSACTION TYPE IDENTIFIER",
                "REQUIRED": "X (error)",
                "RULE_REFERENCE": "FEDERAL_ELECTION_ACTIVITY_CREDIT_CARD_PAYMENT_MEMO, FEDERAL_ELECTION_ACTIVITY_STAFF_REIMBURSEMENT_MEMO, FEDERAL_ELECTION_ACTIVITY_PAYMENT_TO_PAYROLL_MEMO"
            }
        },
        "transaction_id": {
            "title": "TRANSACTION ID",
            "description": "",
            "type": "string",
            "minLength": 0,
            "maxLength": 20,
            "pattern": "^[ -~]{0,20}$",
            "examples": [
                "B56123456789-1234"
            ],
            "fec_spec": {
                "FIELD_DESCRIPTION": "TRANSACTION ID",
                "TYPE": "A/N-20",
                "REQUIRED": "X (error)",
                "SAMPLE_DATA": "B56123456789-1234",
                "RULE_REFERENCE": "must be unique for the life of the report (original + all amendments)"
            }
        },
        "back_reference_tran_id_number": {
            "title": "BACK REFERENCE TRAN ID NUMBER",
            "description": "",
            "type": "string",
            "minLength": 0,
            "maxLength": 20,
            "pattern": "^[ -~]{0,20}$",
            "examples": [
                "B123456789-1234"
            ],
            "fec_spec": {
                "FIELD_DESCRIPTION": "BACK REFERENCE TRAN ID NUMBER",
                "TYPE": "A/N-20",
                "REQUIRED": "X (error)",
                "SAMPLE_DATA": "B123456789-1234"
            }
        },
        "back_reference_sched_name": {
            "title": "BACK REFERENCE SCHED NAME",
            "description": "",
            "type": "string",
            "minLength": 0,
            "maxLength": 8,
            "pattern": "^[ -~]{0,8}$",
            "examples": [
                "SB21"
            ],
            "fec_spec": {
                "FIELD_DESCRIPTION": "BACK REFERENCE SCHED NAME",
                "TYPE": "A/N-8",
                "REQUIRED": "X (error)",
                "SAMPLE_DATA": "SB21",
                "VALUE_REFERENCE": "SB[line# ref]"
            }
        },
        "entity_type": {
            "title": "ENTITY TYPE",
            "description": "",
            "enum": [
                "IND",
                "ORG",
                "COM"
            ],
            "examples": [
                "IND"
            ],
            "fec_spec": {
                "FIELD_DESCRIPTION": "ENTITY TYPE",
                "TYPE": "A/N-3",
                "REQUIRED": "X (error)",
                "SAMPLE_DATA": "IND, ORG",
                "VALUE_REFERENCE": "[IND | ORG | COM]",
                "RULE_REFERENCE": "[IND | ORG | COM]"
            }
        },
        "payee_organization_name": {
            "title": "PAYEE ORGANIZATION NAME",
            "description": "",
            "type": [
                "string",
                "null"
            ],
            "minLength": 0,
            "maxLength": 200,
            "pattern": "^[ -~]{0,200}$",
            "examples": [
                "John Smith & Co."
            ],
            "fec_spec": {
                "FIELD_DESCRIPTION": "PAYEE ORGANIZATION NAME",
                "TYPE": "A/N-200",
                "REQUIRED": "X (conditional error)",
                "SAMPLE_DATA": "John Smith & Co.",
                "RULE_REFERENCE": "Required if [ORG | COM]"
            }
        },
        "payee_last_name": {
            "title": "PAYEE LAST NAME",
            "description": "",
            "type": [
                "string",
                "null"
            ],
            "minLength": 0,
            "maxLength": 30,
            "pattern": "^[ -~]{0,30}$",
            "examples": [
                "Smith"
            ],
            "fec_spec": {
                "FIELD_DESCRIPTION": "PAYEE LAST NAME",
                "TYPE": "A/N-30",
                "REQUIRED": "X (conditional error)",
                "SAMPLE_DATA": "Smith",
                "RULE_REFERENCE": "Required if [IND]"
            }
        },
        "payee_first_name": {
            "title": "PAYEE FIRST NAME",
            "description": "",
            "type": [
                "string",
                "null"
            ],
            "minLength": 0,
            "maxLength": 20,
            "pattern": "^[ -~]{0,20}$",
            "examples": [
                "John"
            ],
            "fec_spec": {
                "FIELD_DESCRIPTION": "PAYEE FIRST NAME",
                "TYPE": "A/N-20",
                "REQUIRED": "X (conditional error)",
                "SAMPLE_DATA": "John",
                "RULE_REFERENCE": "Required if [IND]"
            }
        },
        "payee_middle_name": {
            "title": "PAYEE MIDDLE NAME",
            "description": "",
            "type": [
                "string",
                "null"
            ],
            "minLength": 0,
            "maxLength": 20,
            "pattern": "^[ -~]{0,20}$",
            "examples": [
                "W"
            ],
            "fec_spec": {
                "FIELD_DESCRIPTION": "PAYEE MIDDLE NAME",
                "TYPE": "A/N-20",
                "SAMPLE_DATA": "W"
            }
        },
        "payee_prefix": {
            "title": "PAYEE PREFIX",
            "description": "",
            "type": [
                "string",
                "null"
            ],
            "minLength": 0,
            "maxLength": 10,
            "pattern": "^[ -~]{0,10}$",
            "examples": [
                "Dr"
            ],
            "fec_spec": {
                "FIELD_DESCRIPTION": "PAYEE PREFIX",
                "TYPE": "A/N-10",
                "SAMPLE_DATA": "Dr"
            }
        },
        "payee_suffix": {
            "title": "PAYEE SUFFIX",
            "description": "",
            "type": [
                "string",
                "null"
            ],
            "minLength": 0,
            "maxLength": 10,
            "pattern": "^[ -~]{0,10}$",
            "examples": [
                "Jr"
            ],
            "fec_spec": {
                "FIELD_DESCRIPTION": "PAYEE SUFFIX",
                "TYPE": "A/N-10",
                "SAMPLE_DATA": "Jr"
            }
        },
        "payee_street_1": {
            "title": "PAYEE STREET  1",
            "description": "",
            "type": "string",
            "minLength": 0,
            "maxLength": 34,
            "pattern": "^[ -~]{0,34}$",
            "examples": [
                "Suite 16"
            ],
            "fec_spec": {
                "FIELD_DESCRIPTION": "PAYEE STREET  1",
                "TYPE": "A/N-34",
                "REQUIRED": "X (error)",
                "SAMPLE_DATA": "Suite 16"
            }
        },
        "payee_street_2": {
            "title": "PAYEE STREET  2",
            "description": "",
            "type": [
                "string",
                "null"
            ],
            "minLength": 0,
            "maxLength": 34,
            "pattern": "^[ -~]{0,34}$",
            "examples": [
                "30 Oak Street"
            ],
            "fec_spec": {
                "FIELD_DESCRIPTION": "PAYEE STREET  2",
                "TYPE": "A/N-34",
                "SAMPLE_DATA": "30 Oak Street"
            }
        },
        "payee_city": {
            "title": "PAYEE CITY",
            "description": "",
            "type": "string",
            "minLength": 0,
            "maxLength": 30,
            "pattern": "^[ -~]{0,30}$",
            "examples": [
                "Springfield"
            ],
            "fec_spec": {
                "FIELD_DESCRIPTION": "PAYEE CITY",
                "TYPE": "A/N-30",
                "REQUIRED": "X (error)",
                "SAMPLE_DATA": "Springfield"
            }
        },
        "payee_state": {
            "title": "PAYEE STATE",
            "description": "",
            "type": "string",
            "minLength": 0,
            "maxLength": 2,
            "pattern": "^[ -~]{0,2}$",
            "examples": [
                "MA"
            ],
            "fec_spec": {
                "FIELD_DESCRIPTION": "PAYEE STATE",
                "TYPE": "A/N-2",
                "REQUIRED": "X (error)",
                "SAMPLE_DATA": "MA"
            }
        },
        "payee_zip": {
            "title": "PAYEE ZIP",
            "description": "",
            "type": "string",
            "minLength": 0,
            "maxLength": 9,
            "pattern": "^[ -~]{0,9}$",
            "examples": [
                1012
            ],
            "fec_spec": {
                "FIELD_DESCRIPTION": "PAYEE ZIP",
                "TYPE": "A/N-9",
                "REQUIRED": "X (error)",
                "SAMPLE_DATA": 1012
            }
        },
        "election_code": {
            "title": "ELECTION CODE",
            "description": "",
            "type": "string",
            "minLength": 0,
            "maxLength": 5,
            "pattern": "^[ -~]{0,5}$",
            "examples": [
                "P2012"
            ],
            "fec_spec": {
                "FIELD_DESCRIPTION": "ELECTION CODE",
                "TYPE": "A/N-5",
                "REQUIRED": "X (error)",
                "SAMPLE_DATA": "P2012",
                "VALUE_REFERENCE": "G,P,O[YYYY]",
                "RULE_REFERENCE": "Values: [G|P|R|S|C|E|O]+Year{YYYY}"
            }
        },
        "election_other_description": {
            "title": "ELECTION OTHER DESCRIPTION",
            "description": "",
            "type": [
                "string",
                "null"
            ],
            "minLength": 0,
            "maxLength": 20,
            "pattern": "^[ -~]{0,20}$",
            "fec_spec": {
                "FIELD_DESCRIPTION": "ELECTION OTHER DESCRIPTION",
                "TYPE": "A/N-20",
                "REQUIRED": "X (conditional error)",
                "RULE_REFERENCE": "Req if Item Election Code = O"
            }
        },
        "expenditure_date": {
            "title": "EXPENDITURE DATE",
            "description": "",
            "type": "string",
            "minLength": 10,
            "pattern": "^[0-9]{4}-[0-9]{2}-[0-9]{2}$",
            "examples": [
                "2012-07-20"
            ],
            "fec_spec": {
                "FIELD_DESCRIPTION": "EXPENDITURE DATE",
                "TYPE": "NUM-8",
                "REQUIRED": "X (error)",
                "SAMPLE_DATA": 20120720,
                "VALUE_REFERENCE": "YYYYMMDD"
            }
        },
        "expenditure_amount": {
            "title": "EXPENDITURE AMOUNT",
            "description": "",
            "type": "number",
            "minimum": -99999999.99,
            "maximum": 999999999.99,
            "examples": [
                1500
            ],
            "fec_spec": {
                "FIELD_DESCRIPTION": "EXPENDITURE AMOUNT",
                "TYPE": "AMT-12",
                "REQUIRED": "X (error)",
                "SAMPLE_DATA": 1500
            }
        },
        "aggregate_amount": {
            "title": "AGGREGATE AMOUNT",
            "description": "",
            "type": "number",
            "minimum": -99999999.99,
            "maximum": 999999999.99,
            "examples": [
                1000
            ],
            "fec_spec": {
                "FIELD_DESCRIPTION": "AGGREGATE AMOUNT",
                "REQUIRED": "X (error)",
                "SAMPLE_DATA": 1000
            }
        },
        "aggregation_group": {
            "title": "AGGREGATION GROUP",
            "description": "",
            "const": "GENERAL_DISBURSEMENT",
            "examples": [
                "GENERAL_DISBURSEMENT"
            ],
            "fec_spec": {
                "FIELD_DESCRIPTION": "AGGREGATION GROUP",
                "REQUIRED": "X (error)",
                "RULE_REFERENCE": "GENERAL_DISBURSEMENT"
            }
        },
        "expenditure_purpose_descrip": {
            "title": "EXPENDITURE PURPOSE DESCRIP",
            "description": "",
            "type": "string",
            "minLength": 0,
            "maxLength": 100,
            "pattern": "^[ -~]{0,100}$",
            "examples": [
                "Repay Loan"
            ],
            "fec_spec": {
                "FIELD_DESCRIPTION": "EXPENDITURE PURPOSE DESCRIP",
                "TYPE": "A/N-100",
                "REQUIRED": "X (error)"
            }
        },
        "category_code": {
            "title": "CATEGORY CODE",
            "description": "",
            "type": [
                "string",
                "null"
            ],
            "enum": ["001", "002", "003", "004", "005", "006", "007", "008", "009", "010", "011", "012", null],
            "examples": [
                1
            ],
            "fec_spec": {
                "FIELD_DESCRIPTION": "CATEGORY CODE",
                "TYPE": "A/N-3",
                "SAMPLE_DATA": 1,
                "VALUE_REFERENCE": "001-012",
                "RULE_REFERENCE": "Only codes 001-012"
            }
        },
        "beneficiary_candidate_fec_id": {
            "title": "BENEFICIARY CANDIDATE FEC ID",
            "description": "",
            "type": [
                "string",
                "null"
            ],
            "minLength": 0,
            "maxLength": 9,
            "pattern": "^[ -~]{0,9}$",
            "examples": [
                "H98765431"
            ],
            "fec_spec": {
                "FIELD_DESCRIPTION": "BENEFICIARY CANDIDATE FEC ID",
                "TYPE": "A/N-9",
                "SAMPLE_DATA": "H98765431"
            }
        },
        "beneficiary_candidate_last_name": {
            "title": "BENEFICIARY CANDIDATE LAST NAME",
            "description": "",
            "type": [
                "string",
                "null"
            ],
            "minLength": 0,
            "maxLength": 30,
            "pattern": "^[ -~]{0,30}$",
            "fec_spec": {
                "FIELD_DESCRIPTION": "BENEFICIARY CANDIDATE LAST NAME",
                "TYPE": "A/N-30"
            }
        },
        "beneficiary_candidate_first_name": {
            "title": "BENEFICIARY CANDIDATE FIRST NAME",
            "description": "",
            "type": [
                "string",
                "null"
            ],
            "minLength": 0,
            "maxLength": 20,
            "pattern": "^[ -~]{0,20}$",
            "fec_spec": {
                "FIELD_DESCRIPTION": "BENEFICIARY CANDIDATE FIRST NAME",
                "TYPE": "A/N-20"
            }
        },
        "beneficiary_candidate_middle_name": {
            "title": "BENEFICIARY CANDIDATE MIDDLE NAME",
            "description": "",
            "type": [
                "string",
                "null"
            ],
            "minLength": 0,
            "maxLength": 20,
            "pattern": "^[ -~]{0,20}$",
            "fec_spec": {
                "FIELD_DESCRIPTION": "BENEFICIARY CANDIDATE MIDDLE NAME",
                "TYPE": "A/N-20"
            }
        },
        "beneficiary_candidate_prefix": {
            "title": "BENEFICIARY CANDIDATE PREFIX",
            "description": "",
            "type": [
                "string",
                "null"
            ],
            "minLength": 0,
            "maxLength": 10,
            "pattern": "^[ -~]{0,10}$",
            "fec_spec": {
                "FIELD_DESCRIPTION": "BENEFICIARY CANDIDATE PREFIX",
                "TYPE": "A/N-10"
            }
        },
        "beneficiary_candidate_suffix": {
            "title": "BENEFICIARY CANDIDATE SUFFIX",
            "description": "",
