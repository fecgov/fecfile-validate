            ],
            "minLength": 0,
            "maxLength": 10,
            "pattern": "^[ -~]{0,10}$",
            "fec_spec": {
                "FIELD_DESCRIPTION": "BENEFICIARY CANDIDATE SUFFIX",
                "TYPE": "A/N-10"
            }
        },
        "memo_code": {
            "title": "MEMO CODE",
            "description": "",
            "type": "boolean",
            "fec_spec": {
                "FIELD_DESCRIPTION": "MEMO CODE",
                "TYPE": "A/N-1",
                "REQUIRED": "X (error)",
                "VALUE_REFERENCE": "X"
            }
        },
        "memo_text_description": {
            "title": "MEMO TEXT/DESCRIPTION",
            "description": "",
            "type": [
                "string",
                "null"
            ],
            "minLength": 0,
            "maxLength": 100,
            "pattern": "^[ -~]{0,100}$",
            "fec_spec": {
                "FIELD_DESCRIPTION": "MEMO TEXT/DESCRIPTION",
                "TYPE": "A/N-100"
            }
        }
    },
    "allOf": [
        {
            "if": {
                "properties": {
                    "transaction_type_identifier": {
                        "enum": [
                            "FEDERAL_ELECTION_ACTIVITY_CREDIT_CARD_PAYMENT_MEMO",
                            "FEDERAL_ELECTION_ACTIVITY_STAFF_REIMBURSEMENT_MEMO"
                        ]
                    }
                },
                "required": [
                    "transaction_type_identifier"
                ]
            },
            "then": {
                "properties": {
                    "entity_type": {
                        "enum": [
                            "ORG",
                            "IND",
                            "COM"
                        ]
                    }
                }
            }
        },
        {
            "if": {
                "properties": {
                    "transaction_type_identifier": {
                        "enum": [
                            "FEDERAL_ELECTION_ACTIVITY_PAYMENT_TO_PAYROLL_MEMO"
                        ]
                    }
                },
                "required": [
                    "transaction_type_identifier"
                ]
            },
            "then": {
                "properties": {
                    "entity_type": {
                        "enum": [
                            "IND",
                            "ORG",
                            "COM"
                        ]
                    }
                }
            }
        },
        {
            "if": {
                "properties": {
                    "entity_type": {
                        "enum": [
                            "ORG",
                            "COM"
                        ]
                    }
                },
                "required": [
                    "entity_type"
                ]
            },
            "then": {
                "required": [
                    "payee_organization_name"
                ],
                "properties": {
                    "payee_organization_name": {
                        "type": "string"
                    }
                }
            }
        },
        {
            "if": {
                "properties": {
                    "entity_type": {
                        "const": "IND"
                    }
                },
                "required": [
                    "entity_type"
                ]
            },
            "then": {
                "required": [
                    "payee_first_name",
                    "payee_last_name"
                ],
                "properties": {
                    "payee_first_name": {
                        "type": "string"
                    },
                    "payee_last_name": {
                        "type": "string"
                    }
                }
            }
        },
        {
            "if": {
                "properties": {
                    "election_code": {
                        "pattern": "^O\\d{4}$",
                        "type": "string"
                    }
                },
                "required": [
                    "election_code"
                ]
            },
            "then": {
                "required": [
                    "election_other_description"
                ],
                "properties": {
                    "election_other_description": {
                        "type": "string"
                    }
                }
            }
        }
    ]
}