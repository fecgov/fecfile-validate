            "examples": [
                "F24N"
            ],
            "fec_col_seq": 1,
            "fec_form_line": "0",
            "fec_type": "A/N-4",
            "fec_requiredErrorLevel": "X (error)",
            "fec_valueReference": "F24+[N|A]"
