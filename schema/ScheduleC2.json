{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "https://github.com/fecgov/fecfile-Validate/blob/main/schema/ScheduleC2.json",
    "version": "v0.0.0.0",
    "title": "FEC Schedule C2",
    "description": "SCHEDULE C2 - LOAN GUARANTOR NAME & ADDRESS INFORMATION                      \n                            (SUPPLEMENTARY FOR INFORMATION FOUND ON SCHEDULE C)",
    "type": "object",
    "required": [
        "FORM_TYPE",
        "FILER_COMMITTEE_ID_NUMBER",
        "TRANSACTION_ID_NUMBER",
        "BACK_REFERENCE_TRAN_ID_NUMBER",
        "GUARANTOR_LAST_NAME",
        "GUARANTOR_FIRST_NAME"
    ],
    "properties": {
        "FORM_TYPE": {
            "title": "FORM TYPE",
            "description": "",
            "type": "string",
            "minLength": 0,
            "maxLength": 8,
            "pattern": "^[ A-z0-9]{0,8}$",
            "examples": [
                "SC2/9"
            ],
            "spec": {
                "COL_SEQ": 1,
                "FIELD_DESCRIPTION": "FORM TYPE",
                "TYPE": "A/N-8",
                "REQUIRED": "X (error)",
                "AUTO_POPULATE": "X (error)",
                "SAMPLE_DATA": "SC2/9",
                "VALUE_REFERENCE": "SC2/[line# ref]",
                "RULE_REFERENCE": "Appendix C"
            }
        },
        "FILER_COMMITTEE_ID_NUMBER": {
            "title": "FILER COMMITTEE ID NUMBER",
            "description": "",
            "type": "string",
            "minLength": 0,
            "maxLength": 9,
            "pattern": "^[ A-z0-9]{0,9}$",
            "examples": [
                "C00123456"
            ],
            "spec": {
                "COL_SEQ": 2,
                "FIELD_DESCRIPTION": "FILER COMMITTEE ID NUMBER",
                "TYPE": "A/N-9",
                "REQUIRED": "X (error)",
                "AUTO_POPULATE": "X (error)",
                "SAMPLE_DATA": "C00123456"
            }
        },
        "TRANSACTION_ID_NUMBER": {
            "title": "TRANSACTION ID NUMBER",
            "description": "",
            "type": "string",
            "minLength": 0,
            "maxLength": 20,
            "pattern": "^[ A-z0-9]{0,20}$",
            "examples": [
                "C123456789-3456-001"
            ],
            "spec": {
                "COL_SEQ": 3,
                "FIELD_DESCRIPTION": "TRANSACTION ID NUMBER",
                "TYPE": "A/N-20",
                "REQUIRED": "X (error)",
                "AUTO_POPULATE": "X (error)",
                "SAMPLE_DATA": "C123456789-3456-001",
                "VALUE_REFERENCE": "May be a combination of Parent SC/ TranID + a unique ID for this Child SC1/ record",
                "RULE_REFERENCE": "must be unique and UPPER CASE for the life of the report (original + all amendments)"
            }
        },
        "BACK_REFERENCE_TRAN_ID_NUMBER": {
            "title": "BACK REFERENCE TRAN ID NUMBER",
            "description": "",
            "type": "string",
            "minLength": 0,
            "maxLength": 20,
            "pattern": "^[ A-z0-9]{0,20}$",
            "examples": [
                "C123456789-3456"
            ],
            "spec": {
                "COL_SEQ": 4,
                "FIELD_DESCRIPTION": "BACK REFERENCE TRAN ID NUMBER",
                "TYPE": "A/N-20",
                "REQUIRED": "X (error)",
                "AUTO_POPULATE": "X (error)",
                "SAMPLE_DATA": "C123456789-3456",
                "RULE_REFERENCE": "Reference to the Tran ID of a Related SC/{***} Record"
            }
        },
        "GUARANTOR_LAST_NAME": {
            "title": "GUARANTOR LAST NAME",
            "description": "",
            "type": "string",
            "minLength": 0,
            "maxLength": 30,
            "pattern": "^[ A-z0-9]{0,30}$",
            "examples": [
                "Smith"
            ],
            "spec": {
                "COL_SEQ": 5,
                "FIELD_DESCRIPTION": "GUARANTOR LAST NAME",
                "TYPE": "A/N-30",
                "REQUIRED": "X (error)",
                "AUTO_POPULATE": "X (error)",
                "SAMPLE_DATA": "Smith"
            }
        },
        "GUARANTOR_FIRST_NAME": {
            "title": "GUARANTOR FIRST NAME",
            "description": "",
            "type": "string",
            "minLength": 0,
            "maxLength": 20,
            "pattern": "^[ A-z0-9]{0,20}$",
            "examples": [
                "John"
            ],
            "spec": {
                "COL_SEQ": 6,
                "FIELD_DESCRIPTION": "GUARANTOR FIRST NAME",
                "TYPE": "A/N-20",
                "REQUIRED": "X (error)",
                "AUTO_POPULATE": "X (error)",
                "SAMPLE_DATA": "John"
            }
        },
        "GUARANTOR_MIDDLE_NAME": {
            "title": "GUARANTOR MIDDLE NAME",
            "description": "",
            "type": "string",
            "minLength": 0,
            "maxLength": 20,
            "pattern": "^[ A-z0-9]{0,20}$",
            "examples": [
                "W"
            ],
            "spec": {
                "COL_SEQ": 7,
                "FIELD_DESCRIPTION": "GUARANTOR MIDDLE NAME",
                "TYPE": "A/N-20",
                "SAMPLE_DATA": "W"
            }
        },
        "GUARANTOR_PREFIX": {
            "title": "GUARANTOR PREFIX",
            "description": "",
            "type": "string",
            "minLength": 0,
            "maxLength": 10,
            "pattern": "^[ A-z0-9]{0,10}$",
            "examples": [
                "Dr"
            ],
            "spec": {
                "COL_SEQ": 8,
                "FIELD_DESCRIPTION": "GUARANTOR PREFIX",
                "TYPE": "A/N-10",
                "SAMPLE_DATA": "Dr"
            }
        },
        "GUARANTOR_SUFFIX": {
            "title": "GUARANTOR SUFFIX",
            "description": "",
            "type": "string",
            "minLength": 0,
            "maxLength": 10,
            "pattern": "^[ A-z0-9]{0,10}$",
            "examples": [
                "Jr"
            ],
            "spec": {
                "COL_SEQ": 9,
                "FIELD_DESCRIPTION": "GUARANTOR SUFFIX",
                "TYPE": "A/N-10",
                "SAMPLE_DATA": "Jr"
            }
        },
        "GUARANTOR_STREET_1": {
            "title": "GUARANTOR STREET 1",
            "description": "",
            "type": "string",
            "minLength": 0,
            "maxLength": 34,
            "pattern": "^[ A-z0-9]{0,34}$",
            "examples": [
                "123 Main Street"
            ],
            "spec": {
                "COL_SEQ": 10,
                "FIELD_DESCRIPTION": "GUARANTOR STREET 1",
                "TYPE": "A/N-34",
                "REQUIRED": "X (warning)",
                "AUTO_POPULATE": "X (warning)",
                "SAMPLE_DATA": "123 Main Street"
            }
        },
        "GUARANTOR_STREET_2": {
            "title": "GUARANTOR STREET 2",
            "description": "",
            "type": "string",
            "minLength": 0,
            "maxLength": 34,
            "pattern": "^[ A-z0-9]{0,34}$",
            "spec": {
                "COL_SEQ": 11,
                "FIELD_DESCRIPTION": "GUARANTOR STREET 2",
                "TYPE": "A/N-34"
            }
        },
        "GUARANTOR_CITY": {
            "title": "GUARANTOR CITY",
            "description": "",
            "type": "string",
            "minLength": 0,
            "maxLength": 30,
            "pattern": "^[ A-z0-9]{0,30}$",
            "examples": [
                "Anytown"
            ],
            "spec": {
                "COL_SEQ": 12,
                "FIELD_DESCRIPTION": "GUARANTOR CITY",
                "TYPE": "A/N-30",
                "REQUIRED": "X (warning)",
                "AUTO_POPULATE": "X (warning)",
                "SAMPLE_DATA": "Anytown"
            }
        },
        "GUARANTOR_STATE": {
            "title": "GUARANTOR STATE",
            "description": "",
            "type": "string",
            "minLength": 0,
            "maxLength": 2,
            "pattern": "^[ A-z0-9]{0,2}$",
            "examples": [
                "WA"
            ],
            "spec": {
                "COL_SEQ": 13,
                "FIELD_DESCRIPTION": "GUARANTOR STATE",
                "TYPE": "A-2",
                "REQUIRED": "X (warning)",
                "AUTO_POPULATE": "X (warning)",
                "SAMPLE_DATA": "WA",
                "VALUE_REFERENCE": "AK,AL,...,ZZ",
                "RULE_REFERENCE": "Edit: ST"
            }
        },
        "GUARANTOR_ZIP": {
            "title": "GUARANTOR ZIP",
            "description": "",
            "type": "string",
            "minLength": 0,
            "maxLength": 9,
            "pattern": "^[ A-z0-9]{0,9}$",
            "examples": [
                981110123
            ],
            "spec": {
                "COL_SEQ": 14,
                "FIELD_DESCRIPTION": "GUARANTOR ZIP",
                "TYPE": "A/N-9",
                "REQUIRED": "X (warning)",
                "AUTO_POPULATE": "X (warning)",
                "SAMPLE_DATA": 981110123
            }
        },
        "GUARANTOR_EMPLOYER": {
            "title": "GUARANTOR EMPLOYER",
            "description": "",
            "type": "string",
            "minLength": 0,
            "maxLength": 38,
            "pattern": "^[ A-z0-9]{0,38}$",
            "examples": [
                "XYZ Company"
            ],
            "spec": {
                "COL_SEQ": 15,
                "FIELD_DESCRIPTION": "GUARANTOR EMPLOYER",
                "TYPE": "A/N-38",
                "REQUIRED": "X(warn if > $200)",
                "AUTO_POPULATE": "X(warn if > $200)",
                "SAMPLE_DATA": "XYZ Company"
            }
        },
        "GUARANTOR_OCCUPATION": {
            "title": "GUARANTOR OCCUPATION",
            "description": "",
            "type": "string",
            "minLength": 0,
