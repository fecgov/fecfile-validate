{
    "$schema": "https://json-schema.org/draft-07/schema#",
    "$id": "https://github.com/fecgov/fecfile-validate/blob/main/schema/OTHER_COMMITTEE_CONTRIBUTIONS.json",
    "version": "8.3.0.1",
    "title": "FEC Other Committee Contributions",
    "description": "SCHEDULE B - Contribution to Other Committee (Line 23), Void of Contribution to Other Committee (Line 23)",
    "type": "object",
    "required": [
        "form_type",
        "filer_committee_id_number",
        "transaction_type_identifier",
        "transaction_id",
        "entity_type",
        "payee_organization_name",
        "payee_street_1",
        "payee_city",
