            ],
            "fec_spec": {
                "FIELD_DESCRIPTION": "BENEFICIARY COMMITTEE FEC ID",
                "TYPE": "A/N-9",
                "REQUIRED": "X (error)",
                "RULE_REFERENCE": "Must match PAYEE ID",
                "NOTE": "Pull from PAYEE Entity Name/ID."
