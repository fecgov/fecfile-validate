            "title": "EXPENDITURE AMOUNT",
            "description": "",
            "type": "number",
            "minimum": -99999999.99,
            "maximum": 999999999.99,
            "examples": [
                1500
            ],
            "fec_spec": {
                "FIELD_DESCRIPTION": "EXPENDITURE AMOUNT",
                "TYPE": "AMT-12",
                "REQUIRED": "X (error)",
                "SAMPLE_DATA": 1500,
                "VALUE_REFERENCE": null,
                "RULE_REFERENCE": "If Transaction Type is CONTRIBUTION_TO_OTHER_COMMITTEE_VOID Then this value must be a negative amount",
                "FIELD_FORM_ASSOCIATION": "F3 | F3X | F3P | F3L"
            }
        },
        "expenditure_purpose_descrip": {
            "title": "EXPENDITURE PURPOSE DESCRIP",
            "description": "",
            "type": [
                "string",
                "null"
            ],
            "minLength": 1,
            "maxLength": 100,
            "pattern": "^[ -~]{0,100}$",
            "fec_spec": {
                "FIELD_DESCRIPTION": "EXPENDITURE PURPOSE DESCRIPTION",
                "TYPE": "A/N-100",
