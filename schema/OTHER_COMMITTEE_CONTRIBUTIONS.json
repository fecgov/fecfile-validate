        "transaction_type_identifier": {
            "title": "TRANSACTION TYPE IDENTIFIER",
            "description": "",
            "enum": [
                "CONTRIBUTION_TO_OTHER_COMMITTEE",
                "CONTRIBUTION_TO_OTHER_COMMITTEE_VOID"
            ],
            "examples": [
                "CONTRIBUTION_TO_OTHER_COMMITTEE"
            ],
            "fec_spec": {
                "FIELD_DESCRIPTION": "TRANSACTION TYPE IDENTIFIER",
                "TYPE": null,
                "REQUIRED": "X (error)",
                "SAMPLE_DATA": null,
                "VALUE_REFERENCE": null,
                "RULE_REFERENCE": "CONTRIBUTION_TO_OTHER_COMMITTEE, CONTRIBUTION_TO_OTHER_COMMITTEE_VOID",
                "FIELD_FORM_ASSOCIATION": null
            }
        },
        "transaction_id": {
            "title": "TRANSACTION ID",
            "description": "",
            "type": "string",
            "minLength": 1,
            "maxLength": 20,
            "pattern": "^[ -~]{0,20}$",
            "examples": [
                "B56123456789-1234"
            ],
            "fec_spec": {
                "FIELD_DESCRIPTION": "TRANSACTION ID",
                "TYPE": "A/N-20",
                "REQUIRED": "X (error)",
                "SAMPLE_DATA": "B56123456789-1234",
                "VALUE_REFERENCE": null,
                "RULE_REFERENCE": "must be unique and UPPER CASE for the life of the report (original + all amendments)",
                "FIELD_FORM_ASSOCIATION": "F3 | F3X | F3P | F3L"
            }
        },
        "back_reference_tran_id_number": {
            "title": "BACK REFERENCE TRAN ID NUMBER",
            "description": "",
            "type": [
                "string",
                "null"
            ],
            "minLength": 0,
            "maxLength": 20,
            "pattern": "^[ -~]{0,20}$",
            "examples": [
                "B123456789-1234"
            ],
            "fec_spec": {
                "FIELD_DESCRIPTION": "BACK REFERENCE TRAN ID NUMBER",
                "TYPE": "A/N-20",
                "REQUIRED": null,
                "SAMPLE_DATA": "B123456789-1234",
                "VALUE_REFERENCE": null,
                "RULE_REFERENCE": null,
                "FIELD_FORM_ASSOCIATION": "F3 | F3X | F3P | F3L"
