            "fec_spec": {
                "FIELD_DESCRIPTION": "PAYEE ZIP",
                "TYPE": "A/N-9",
                "REQUIRED": "X (error)",
                "SAMPLE_DATA": 1012,
                "VALUE_REFERENCE": null,
                "RULE_REFERENCE": null,
