    "allOf": [
        {
            "if": {
                "properties": {
                    "transaction_type_identifier": {
                        "const": "CONTRIBUTION_TO_OTHER_COMMITTEE_VOID"
                    }
                },
                "required": [
                    "transaction_type_identifier"
                ]
            },
            "then": {
                "required": [
                    "expenditure_purpose_descrip"
                ],
                "properties": {
                    "expenditure_amount": {
                        "type": "number",
                        "exclusiveMaximum": 0
                    },
                    "expenditure_purpose_descrip": {
                        "type": "string"
                    }
                }
            }
        }
    ]
