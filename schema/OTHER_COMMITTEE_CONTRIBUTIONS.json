        "expenditure_date": {
            "title": "EXPENDITURE DATE",
            "description": "",
            "type": "string",
            "minLength": 10,
            "pattern": "^[0-9]{4}-[0-9]{2}-[0-9]{2}$",
            "examples": [
                "2012-07-20"
            ],
            "fec_spec": {
                "FIELD_DESCRIPTION": "EXPENDITURE DATE",
                "TYPE": "NUM-8",
                "REQUIRED": "X (error)",
                "SAMPLE_DATA": 20120720,
                "VALUE_REFERENCE": "YYYYMMDD",
                "RULE_REFERENCE": null,
                "FIELD_FORM_ASSOCIATION": "F3 | F3X | F3P"
            }
