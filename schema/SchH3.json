{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "https://github.com/mjtravers/fecfile-online-sandbox/blob/main/validator/schema/SchH3.json",
    "title": "FEC Sch H3",
    "description": "SCHEDULE H3 - TRANSFERS FROM NON-FEDERAL ACCOUNTS",
    "type": "object",
    "properties": {
        "FORM_TYPE": {
            "title": "FORM TYPE",
            "description": "",
            "type": "string",
            "minLength": 0,
            "maxLength": 8,
            "pattern": "^[ A-z0-9]{0,8}$",
            "examples": [
                "H3"
            ],
            "fec_col_seq": 1,
            "fec_form_line": "0",
            "fec_type": "A/N-8 ",
            "fec_requiredErrorLevel": "X (error)",
            "fec_valueReference": "H3",
            "fec_ruleReference": "Appendix C"
        },
        "FILER_COMMITTEE_ID_NUMBER": {
            "title": "FILER COMMITTEE ID NUMBER",
            "description": "",
            "type": "string",
            "minLength": 0,
            "maxLength": 9,
            "pattern": "^[ A-z0-9]{0,9}$",
            "examples": [
                "C00123456"
            ],
            "fec_col_seq": 2,
            "fec_form_line": "0",
            "fec_type": "A/N-9 ",
            "fec_requiredErrorLevel": "X (error)"
        },
        "TRANSACTION_ID_NUMBER": {
            "title": "TRANSACTION ID NUMBER",
            "description": "",
            "type": "string",
            "minLength": 0,
            "maxLength": 20,
            "pattern": "^[ A-z0-9]{0,20}$",
            "examples": [
                "H21234-3456"
            ],
            "fec_col_seq": 3,
            "fec_form_line": "0",
            "fec_type": "A/N-20",
            "fec_requiredErrorLevel": "X (error)",
            "fec_ruleReference": "must be unique and UPPER CASE for the life of the report (original + all amendments)"
        },
        "BACK_REFERENCE_TRAN_ID": {
            "title": "BACK REFERENCE TRAN ID",
            "description": "",
            "type": "string",
            "minLength": 0,
            "maxLength": 20,
            "pattern": "^[ A-z0-9]{0,20}$",
            "examples": [
                "H31234-1234"
            ],
            "fec_col_seq": 4,
            "fec_form_line": "0",
            "fec_type": "A/N-20",
            "fec_requiredErrorLevel": "X (error)",
            "fec_valueReference": "Ref to TRAN ID of Type=AD record"
        },
        "ACCOUNT_NAME": {
            "title": "ACCOUNT NAME",
            "description": "",
            "type": "string",
            "minLength": 0,
            "maxLength": 90,
            "pattern": "^[ A-z0-9]{0,90}$",
            "fec_col_seq": 5,
            "fec_form_line": "0",
            "fec_type": "A/N-90",
            "fec_requiredErrorLevel": "X (error)",
            "fec_ruleReference": "Repeat all recs"
        },
        "EVENT_TYPE": {
            "title": "EVENT TYPE",
            "description": "",
            "type": "string",
            "minLength": 0,
            "maxLength": 2,
            "pattern": "^[ A-z0-9]{0,2}$",
            "examples": [
                "DF"
            ],
            "fec_col_seq": 6,
            "fec_form_line": "0",
            "fec_type": "A/N-2",
            "fec_requiredErrorLevel": "X (error)",
            "fec_valueReference": "Post-BCRA code values: AD=ADministrative;\nGV=Generic Voter Drive;\nDF=Direct Fundraising;\nDC=Direct Candidate Support;\nEA=Exempt Activities;\nPC=Public Communications Referring\n      Only to Party (made by PAC)",
            "fec_ruleReference": "Note: H3 records for all Event Types must have a Back Reference Tran ID (field #4) that points to the Transaction ID (field #3) of the ADministrative H3 record. {One AD record is required and any number of \"non-AD\" records are optional.}"
        },
        "EVENT/ACTIVITY_ID/NAME": {
            "title": "EVENT/ACTIVITY ID/NAME",
            "description": "",
            "type": "string",
            "minLength": 0,
            "maxLength": 90,
            "pattern": "^[ A-z0-9]{0,90}$",
            "fec_col_seq": 7,
            "fec_form_line": "0",
            "fec_type": "A/N-90",
            "fec_requiredErrorLevel": "X (warn if Type = [DF|DC])"
        },
        "RECEIPT_DATE": {
            "title": "RECEIPT DATE",
            "description": "",
            "type": "integer",
            "minimum": 0,
            "maximum": 99999999,
            "examples": [
                20120214
            ],
            "fec_col_seq": 8,
            "fec_form_line": "0",
            "fec_type": "NUM-8",
            "fec_requiredErrorLevel": "X (error)",
            "fec_valueReference": "YYYYMMDD",
            "fec_ruleReference": "Repeat all recs"
        },
        "TOTAL_AMOUNT_TRANSFERRED": {
            "title": "TOTAL AMOUNT TRANSFERRED",
            "description": "",
            "type": "number",
            "minimum": 0,
            "maximum": 999999999999,
            "fec_col_seq": 9,
            "fec_form_line": "0",
            "fec_type": "AMT-12",
            "fec_requiredErrorLevel": "X (error)",
            "fec_ruleReference": "Repeat all recs"
        },
        "TRANSFERRED_AMOUNT": {
            "title": "TRANSFERRED AMOUNT",
            "description": "",
            "type": "number",
            "minimum": 0,
            "maximum": 999999999999,
            "fec_col_seq": 10,
            "fec_form_line": "0",
            "fec_type": "AMT-12",
            "fec_requiredErrorLevel": "X (error)",
            "fec_ruleReference": "Breakdown Amt for Items: i, ii, iii, iv)a), iv)b), ... v)a), v)b), ... & vi."
        }
    },
    "additionalProperties": false,
    "required": [
        "FORM_TYPE",
        "FILER_COMMITTEE_ID_NUMBER",
        "TRANSACTION_ID_NUMBER",
        "BACK_REFERENCE_TRAN_ID",
        "ACCOUNT_NAME",
        "EVENT_TYPE",
        "RECEIPT_DATE",
        "TOTAL_AMOUNT_TRANSFERRED",
        "TRANSFERRED_AMOUNT"
    ]
}