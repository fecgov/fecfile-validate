{
  "root": true,
  "ignorePatterns": [
    "projects/**/*",
    "src/validate-*.js"
  ],
  "overrides": [
    {
      "files": [
        "src/*.ts",
        "./**/*.js",
        "./**/*.cjs"
      ],
      "parser": "@babel/eslint-parser",
      "parserOptions": {
        "requireConfigFile": false
      },
      "extends": [
        "eslint:recommended"
      ]
    }
  ],
  "env": {
    "jasmine": true,
    "node": true
  }
}
